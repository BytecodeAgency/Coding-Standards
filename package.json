{
  "name": "bytecode-coding-standards",
  "version": "1.1.0",
  "description": "Example package.json file, in line with our coding standards and front end dev env",
  "main": "server.js",
  "repository": "git@github.com:BytecodeBV/Coding-Standards.git",
  "author": "Bytecode Digital Agency B.V. <info@bytecode.nl>",
  "license": "GPL-3.0",
  "private": false,
  "scripts": {
    "start": "node server.js",
    "dev": "parcel src/index.hbs --no-cache",
    "build": "parcel build src/index.hbs --no-cache",
    "build:ts": "parcel build src/ts/main.ts --no-cache --public-url ./",
    "esdoc": "./node_modules/.bin/esdoc && open ./docs/index.html",
    "lint": "yarn run lint:sass && yarn run lint:js",
    "lint:sass": "sass-lint -v",
    "lint:js": "eslint --ext .jsx,.js,.ts .",
    "reinstall": "rm -rf node_modules && yarn"
  },
  "dependencies": {
    "express": "^4.17.1",
    "sass-lint": "^1.13.1",
    "tslint": "^5.16.0"
  },
  "devDependencies": {
    "animate.css": "^3.7.1",
    "autoprefixer": "^9.5.1",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.1",
    "babel-plugin-transform-decorators": "^6.24.1",
    "babel-preset-env": "^1.7.0",
    "esdoc": "^1.1.0",
    "esdoc-standard-plugin": "^1.0.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-jquery": "^1.5.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.13.0",
    "eslint-plugin-security": "^1.4.0",
    "handlebars-layouts": "^3.1.4",
    "jquery": "^3.4.1",
    "jquery.nicescroll": "^3.7.6",
    "node-sass": "^4.12.0",
    "parcel-bundler": "^1.12.3",
    "parcel-plugin-handlebars": "^0.4.4",
    "path": "^0.12.7",
    "postcss": "^7.0.16",
    "postcss-modules": "^1.4.1",
    "prettier": "^1.17.1",
    "prettier-eslint": "^8.8.2",
    "prettier-tslint": "^0.4.2",
    "prettierrc": "^0.0.0",
    "tslint-config-airbnb": "^5.11.1",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-react": "^4.0.0",
    "typescript": "^3.5.1"
  }
}
